{
  "version": 2,
  "buildCommand": "echo 'Static website - no build required'",
  "outputDirectory": ".",
  "installCommand": "echo 'No install needed'",
  "devCommand": "echo 'No dev command needed'",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/terms",
      "dest": "/terms.html"
    },
    {
      "src": "/privacy",
      "dest": "/privacy.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}